{
  "preset": "jest-expo",
  "moduleNameMapper": {
    "@/components/(.*)": "<rootDir>/src/components/$1",
    "@/ui/(.*)": "<rootDir>/src/ui/$1",
    "@/util/(.*)": "<rootDir>/src/util/$1",
    "@/backend/(.*)": "<rootDir>/src/backend/$1",
    "@/types": "<rootDir>/src/types",
    "@/types/(.*)": "<rootDir>/src/types/$1"
  }
}